{
    "name": "arokettu/composer-menu",
    "type": "composer-plugin",
    "description": "Console menu for Composer",
    "license": "0BSD",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "class": "Arokettu\\ComposerMenu\\ComposerMenuPlugin"
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\ComposerMenu\\": "."
        }
    },
    "require": {
        "php": ">= 7.1.3",
        "composer-plugin-api": "^1 | ^2",
        "arokettu/console-menu": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1 | ^2"
    }
}
